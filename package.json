{
  "name": "nextjs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "nodemon",
    "build:server": "./node_modules/.bin/ts-node build.ts --project tsconfig.server.json",
    "build:next": "next build",
    "build": "npm run build:server && npm run build:next",
    "start": "node -r module-alias/register ./dist/server/src --env=production",
    "lint": "next lint",
    "lint:server": "npx eslint -c server/.eslintrc.json --ext .ts server/src/"
  },
  "dependencies": {
    "@cloudinary/react": "^1.11.2",
    "@cloudinary/url-gen": "^1.10.0",
    "@reduxjs/toolkit": "^1.9.3",
    "@tanstack/react-query": "^4.28.0",
    "apicache": "^1.6.3",
    "autoprefixer": "10.4.14",
    "axios": "^1.3.5",
    "bcrypt": "^5.1.0",
    "classnames": "^2.3.2",
    "cloudinary": "^1.35.0",
    "command-line-args": "^5.2.1",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "eslint": "8.38.0",
    "eslint-config-next": "13.3.0",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "express-winston": "^4.2.0",
    "helmet": "^6.2.0",
    "jet-validator": "^1.0.4",
    "jsonfile": "^6.1.0",
    "jsonwebtoken": "^9.0.0",
    "memory-cache": "^0.2.0",
    "module-alias": "^2.2.2",
    "mongoose": "^7.0.2",
    "morgan": "^1.10.0",
    "next": "13.3.0",
    "postcss": "8.4.21",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-redux": "^8.0.5",
    "sass": "^1.61.0",
    "tailwindcss": "3.3.1",
    "uuid": "^9.0.0",
    "validator-checker-js": "^1.0.3",
    "winston": "^3.8.2",
    "winston-mongodb": "^5.1.1"
  },
  "devDependencies": {
    "@types/apicache": "^1.6.1",
    "@types/bcrypt": "^5.0.0",
    "@types/command-line-args": "^5.2.0",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/find": "^0.2.1",
    "@types/fs-extra": "^11.0.1",
    "@types/jasmine": "^4.3.1",
    "@types/jsonfile": "^6.1.1",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/memory-cache": "^0.2.2",
    "@types/morgan": "^1.9.4",
    "@types/node": "18.15.11",
    "@types/react": "18.0.33",
    "@types/react-dom": "18.0.11",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "^5.55.0",
    "eslint": "^8.36.0",
    "eslint-plugin-node": "^11.1.0",
    "find": "^0.3.0",
    "fs-extra": "^11.1.0",
    "jasmine": "^4.5.0",
    "nodemon": "^2.0.21",
    "supertest": "^6.3.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.2",
    "typescript": "5.0.4"
  },
  "nodemonConfig": {
    "watch": [
      "./server/src"
    ],
    "ext": "ts",
    "ignore": [
      "./server/src/public"
    ],
    "exec": "./node_modules/.bin/ts-node --project tsconfig.server.json --files -r tsconfig-paths/register ./server/src"
  },
  "_moduleAliases": {
    "@serv": "dist/server/src",
    "@src": "dist/src"
  },
  "engines": {
    "node": ">=11.0.0"
  }
}